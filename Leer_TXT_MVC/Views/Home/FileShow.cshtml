
@{
    ViewBag.Title = "Resultados";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Leer_TXT_MVC.Models
@model IEnumerable<Result>



<div>

    @if (Model.Count() > 0)
    {
        <hr />
        <table cellpadding="0" cellspacing="0" class="table">
            <tr>
                <th>Registrado</th>
            </tr>
            @foreach (Result result in Model)
            {
                <tr>
                    <td>@result.Text</td>
                </tr>
            }
        </table>
    }
    else
    {
        <h3>No se han procesados los archivos o registros</h3>
    }

</div>

